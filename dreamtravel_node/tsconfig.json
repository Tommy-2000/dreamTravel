// Equivalent to a config of: node22
{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node LTS (22)",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": [
      "es2024",
      "ESNext.Array",
      "ESNext.Collection",
      "ESNext.Iterator"
    ],
    "typeRoots": [
      "./custom_types",
      "./node_modules/@types"
    ],
    "outDir": "output",
    "module": "nodenext",
    "target": "es2022",
    "strict": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  }
}